{
    "name": "inisiatif/common",
    "description": "Common package using in Inisiatif Zakat Indonesia",
    "license": "proprietary",
    "type": "library",
    "repositories": [
        {
            "type": "composer",
            "url": "https://repo.packagist.com/inisiatif/"
        },
        {
            "packagist.org": false
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "ext-json": "*",
        "illuminate/http": "^7.30|^8.26",
        "illuminate/support": "^7.30|^8.26",
        "laminas/laminas-hydrator": "^4.1",
        "ramsey/uuid": "^4.0"
    },
    "require-dev": {
        "dg/bypass-finals": "^1.3",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Inisiatif\\Package\\Common\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Inisiatif\\Package\\Common\\Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
